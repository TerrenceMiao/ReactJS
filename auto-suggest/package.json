{
    "name": "Postal-Address-auto-suggest",
    "version": "0.0.1",
    "description": "Postal Address Auto Suggest example of using React",
    "private": true,
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/TerrenceMiao/ReactJS.git"
    },
    "scripts": {
        "lint": "eslint js/app.js js/google-maps.js",
        "test": "cross-env NODE_ENV=test mocha --recursive --compilers js:babel-register --require ./test/setup.js",
        "test:watch": "npm test -- --watch",
        "prebuild": "npm run lint && npm test",
        "build": "browserify ./js/app.js -t babelify -o compiled.app.js",
        "watch": "watchify ./js/app.js -v -t babelify -o compiled.app.js",
        "start": "npm run watch & node server",
        "copy-static-files": "rm -rf dist && mkdir dist && cp -a index.html css data images dist",
        "compile": "babel js -d dist/js",
        "standalone": "npm run copy-static-files && npm run compile && cross-env BABEL_ENV=development webpack --config webpack.dev.config.js"
    },
    "dependencies": {
        "prop-types": "^15.6.0"
    },
    "peerDependencies": {
        "react": ">=0.14.7"
    },
    "devDependencies": {
        "async": "^2.0.1",
        "autoprefixer": "^6.7.7",
        "autosuggest-highlight": "^3.1.0",
        "axios": "^0.13.1",
        "babelify": "^7.3.0",
        "babel-cli": "^6.24.1",
        "babel-core": "^6.25.0",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^6.4.1",
        "babel-plugin-react-transform": "^2.0.2",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.6",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.24.1",
        "browserify": "^13.1.0",
        "chai": "^3.5.0",
        "cross-env": "^3.1.4",
        "css-loader": "^0.28.4",
        "es6-promise": "^4.1.1",
        "eslint": "^3.19.0",
        "eslint-plugin-mocha": "^4.3.0",
        "eslint-plugin-react": "^6.10.3",
        "extract-text-webpack-plugin": "^1.0.1",
        "expect": "^1.8.0",
        "file-loader": "^0.11.2",
        "husky": "^0.13.4",
        "ismobilejs": "^0.4.1",
        "isomorphic-fetch": "^2.2.1",
        "json-loader": "^0.5.7",
        "jsdom": "^9.12.0",
        "less": "^2.7.2",
        "less-loader": "^2.2.3",
        "lint-staged": "^3.6.1",
        "mocha": "^3.4.2",
        "nyc": "^10.1.2",
        "openurl": "^1.1.1",
        "postcss-loader": "^1.3.3",
        "prettier": "^1.5.3",
        "react": "^15.6.1",
        "react-addons-test-utils": "^15.6.2",
        "react-autosuggest": "^9.3.2",
        "react-autowhatever": "^10.1.0",
        "react-dom": "^15.6.1",
        "react-transform-hmr": "^1.0.4",
        "shallow-equal": "^1.0.0",
        "sinon": "^1.17.7",
        "sinon-chai": "^2.11.0",
        "style-loader": "^0.18.2",
        "svgo": "^0.7.2",
        "svgo-loader": "^1.2.1",
        "sync-request": "4.1.0",
        "url-loader": "^0.5.9",
        "watchify": "^3.7.0",
        "webpack": "^1.14.0",
        "webpack-dev-server": "^1.16.2"
    },
    "files": [
    ],
    "lint-staged": {
        ".*.js": [
            "npm run prettier",
            "git add"
        ],
        "*.js": [
            "npm run prettier",
            "git add"
        ],
        "demo/src/**/*.js": [
            "npm run prettier",
            "git add"
        ],
        "demo/standalone/app.js": [
            "npm run prettier",
            "git add"
        ],
        "src/**/*.js": [
            "npm run prettier",
            "git add"
        ],
        "test/**/*.js": [
            "npm run prettier",
            "git add"
        ]
    },
    "keywords": [
        "Postal Address",
        "Address auto-suggest",
        "React applciation"
    ],
    "nyc": {
        "statements": 95,
        "branches": 91,
        "functions": 100,
        "lines": 95,
        "include": [
            "src/*.js"
        ],
        "exclude": [
            "test/**/*.js"
        ],
        "reporter": [
            "lcov",
            "text-summary"
        ],
        "require": [
            "babel-register",
            "./test/setup.js"
        ],
        "check-coverage": true
    },
    "author": "Terrence Miao",
    "homepage": "http://terrencemiao.github.io/",
    "license": "GPLv3"
}
